DRV	= bfe
VERSION	= 1.1.1
PKGREV	=
DISTDIR	= ../${DRV}-${VERSION}${PKGREV}
 
DFLAGS	= -DDEBUG -DDEBUG_LEVEL=0 -DGEM_DEBUG_LEVEL=0 -UTEST_RX_EMPTY \
	  -UTEST_SMALL_DMA_RANGE -UDBG_RXOF -UDBG_NOBURST

CFGFLAGS = -DGEM_DELAYED_START -UCONFIG_COPY_PACKET -UCONFIG_TX_SINGLE \
	  -DGEM_CONFIG_POLLING -DRXTAIL_COALEASE
